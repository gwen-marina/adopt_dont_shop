<% @shelters.each do |shelter| %>
    <h3><%= shelter.name %></h3>
    <p>Created at: <%= shelter.created_at %></p>
<% end %>
<div id="pending">
  <h1> Pending Applicants </h1>
  <% @shelters.pending_apps.each do |shelter| %>
      <h3><%= shelter.name %></h3>
      <p>Created at: <%= shelter.created_at %></p>
  <% end %>
</div>
